pipeline {
    agent any
     tools { 
        maven 'Maven' 
    }
    stages {
     stage('checkout') {
            steps {
               git 'https://github.com/AchrefElmansour/testMAVEN'
            }
        }
        stage('Build') {
            steps {
               bat label: '', script: 'mvn install'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
